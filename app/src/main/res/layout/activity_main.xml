    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="launch"
        android:id="@+id/launchButton"
        android:minWidth="88dp"
        android:minHeight="47dp"
        android:textSize="14sp"
        android:background="@drawable/customfuntion"
        android:layout_below="@+id/button14"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="39dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Return"
        android:id="@+id/returnBtn"
        android:minWidth="88dp"
        android:minHeight="47dp"
        android:textSize="14sp"
        android:background="@drawable/customfuntion"
        android:layout_below="@+id/launchButton"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="30dp" />

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/topicLayout"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/launchButton"
        android:layout_toStartOf="@+id/launchButton">

    </TableLayout>

